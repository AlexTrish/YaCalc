{
  "manifest_version": 3,
  "name": "YaCalc",
  "version": "1.0",
  "description": "Калькулятор для поддержки Яндекс Доставки. Разработал - @AlexTrish.",
  "icons": {
    "16": "YaCalc_Logo.png",
    "48": "YaCalc_Logo.png",
    "128": "YaCalc_Logo.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}